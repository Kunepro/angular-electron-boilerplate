{
	"appId": "com.angular-electron-boilerplate",
	"productName": "Angular Electron Boilerplate",
	"win": {
		"target": "nsis",
		"icon": "icon.ico",
		"artifactName": "${productName}.exe"
	},
	"files": [
		"dist/**/*",
		"node_modules/**/*",
		"icon.ico"
	],
	"asar": true,
	"asarUnpack": [
		"node_modules/better-sqlite3/**/*.node",
		"node_modules/better-sqlite3/**/*.dll"
	],
	"directories": {
		"output": "release"
	},
	"extraResources": []
}
